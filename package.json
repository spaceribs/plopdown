{
  "name": "plopdown",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "web-ext": "web-ext",
    "addons-linter": "addons-linter",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "build:ext": "nx run-many --target=build --projects=browser-action,content-script,options,background,devtool,devtool-panels,plopdown-ext --parallel --prod",
    "start:ext": "nx run-many --target=build --projects=browser-action,content-script,options,background,devtool,devtool-panels --parallel --maxParallel=6 --watch",
    "start:ext-browser": "npm run start",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "ci:lint": "nx affected --target=lint --base=remotes/origin/master --parallel",
    "ci:build": "nx affected --target=build --base=remotes/origin/master --parallel --prod",
    "ci:test": "nx affected --target=test --base=remotes/origin/master --parallel",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "migrate:latest": "nx migrate @nrwl/workspace@latest",
    "migrate:run": "nx migrate --run-migrations=migrations.json",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "ts2json": "ts-json-schema-generator",
    "json2ts": "json2ts",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer"
  },
  "private": true,
