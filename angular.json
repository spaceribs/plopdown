    "plopdown-file": {
      "projectType": "library",
      "root": "libs/plopdown-file",
      "sourceRoot": "libs/plopdown-file/src",
      "prefix": "plopdown",
      "architect": {
        "generate-schema": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "libs/plopdown-file",
            "commands": [
              {
                "command": "ts-json-schema-generator --no-top-ref --tsconfig=tsconfig.ts2json.json --path=src/lib/plopdown-file.model.ts --type=PlopdownFile --out=src/schema/plopdown-file-v1.schema.json"
              },
              {
                "command": "ajv compile -s src/schema/plopdown-file-v1.schema.json -o src/schema/plopdown-file-v1.schema.js"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/plopdown-file/tsconfig.lib.json",
              "libs/plopdown-file/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/plopdown-file/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/plopdown-file/jest.config.js",
            "tsConfig": "libs/plopdown-file/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/plopdown-file/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
