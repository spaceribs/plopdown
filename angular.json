            "aot": true,
            "assets": ["apps/background/src/assets"],
            "styles": [],
            "scripts": [],
            "vendorChunk": false,
            "extractCss": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/background/src/environments/environment.ts",
                  "with": "apps/background/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
