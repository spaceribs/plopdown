    "background": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/background",
      "sourceRoot": "apps/background/src",
      "prefix": "plopdown",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/plopdown-ext/background",
            "index": "apps/background/src/index.html",
            "main": "apps/background/src/main.ts",
            "polyfills": "apps/background/src/polyfills.ts",
            "tsConfig": "apps/background/tsconfig.app.json",
            "assets": ["apps/background/src/assets"],
            "styles": [],
            "scripts": [],
            "allowedCommonJsDependencies": ["tld-extract", "immediate"],
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/background/src/environments/environment.ts",
                  "with": "apps/background/src/environments/environment.prod.ts"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "outputHashing": "none",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "background:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "background:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "background:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/background/**/*.ts",
              "apps/background/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/background/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/background"]
        }
      }
    },
