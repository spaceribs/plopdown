{
  "$schema": "http://json-schema.org/schema",
  "title": "Web Extension Runner",
  "description": "",
  "type": "object",
  "properties": {
    "artifactsDir": {
      "type": "string"
    },
    "browserConsole": {
      "type": "boolean"
    },
    "pref": {
      "type": "object",
      "additionalProperties": true
    },
    "firefox": {
      "type": "string"
    },
    "firefoxProfile": {
      "type": "string"
    },
    "ignoreFiles": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "keepProfileChanges": {
      "type": "boolean"
    },
    "noInput": {
      "type": "boolean"
    },
    "noReload": {
      "type": "boolean"
    },
    "preInstall": {
      "type": "boolean"
    },
    "sourceDir": {
      "type": "string"
    },
    "watchFile": {
      "type": "string"
    },
    "startUrl": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "target": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "args": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "requiredProperties": [
    "sourceDir"
  ]
}